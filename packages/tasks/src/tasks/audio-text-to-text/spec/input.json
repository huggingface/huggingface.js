{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"title": "AudioTextToTextInput",
	"type": "object",
	"properties": {
		"inputs": {
			"type": "string",
			"format": "binary",
			"description": "The audio input to be processed."
