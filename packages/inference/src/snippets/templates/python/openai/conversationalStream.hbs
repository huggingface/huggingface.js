from openai import OpenAI

client = OpenAI(
    base_url="{{{baseUrl}}}",
    api_key="{{{accessToken}}}"
)

messages = {{{messagesStr}}}

stream = client.chat.completions.create(
    model="{{{modelId}}}",
    messages=messages,
    {{{configStr}}}
    stream=True
)

for chunk in stream:
    print(chunk.choices[0].delta.content, end="")
